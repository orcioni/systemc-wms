#ifndef NATURE_HYDRAULIC_H
#define NATURE_HYDRAULIC_H

#include "../wave_system"


struct hydraulic : nature <double>
{
	static const char *across ()  {return "pressure";}
	static const char *through () {return "volume velocity";}
};

#endif // NATURE_HYDRAULIC_H
